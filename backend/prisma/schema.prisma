generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native"]
}

datasource db {
    provider = "postgresql"
    url = env("DATABASE_URL")
}


model Build {
  id Int @id @default(autoincrement())
  date DateTime 
  fullPrice Int
  build Json
}

